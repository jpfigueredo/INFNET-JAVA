		a1.setNotaAV1(200);
		a1.setNotaAV2(30);
		
		Aluno a2 = new Aluno();
		a2.setNotaAV1(200);
		a2.setNotaAV2(30);
		a2.setNome("maria");
		a2.setSobrenome("lola");
	
		Aluno a3 = new Aluno("lauro", "mauro", 7, 10);

		
		Professor p1 = new Professor();
		p1.setNome("lOla");
		p1.setSobrenome("luLi");
		p1.setMateria("Matematica");
		
		Professor p2 = new Professor("Manoel", "Silveira","Filosofia");

	}
}