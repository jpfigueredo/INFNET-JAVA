package testes;

import java.util.ArrayList;
import java.util.List;

import dominio.Professor;
import model.Equipamento;
import model.Ficha;
import model.Guerreiro;
import model.Personagem;
import model.Usuario;

public class FichaTeste {
	public static void main(String[] args) {
	    
		ArrayList<String> listaSkills;

		try {
			Guerreiro personagem = new Guerreiro("Joel",1,200,0,100);
			listaSkills = new ArrayList<String>();
            for(String lista: listaSkills) {
            	listaSkills.add(lista);
            }
			
			personagem.setForca(8);
			personagem.setDefesa(5);
			personagem.setResistencia(5);
			personagem.setMeleeSkills(listaSkills);
			
			
			
			Equipamento equipamento = new Equipamento(null, null, null);
			Usuario usuario = new Usuario(null, null, 0);
				
			Ficha ficha = new Ficha(personagem, equipamento, usuario);
			
			
		}catch() {
			
		}

	
	}

}
