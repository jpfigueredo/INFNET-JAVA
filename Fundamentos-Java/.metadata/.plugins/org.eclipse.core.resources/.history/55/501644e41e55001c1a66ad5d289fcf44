package features;

import java.util.ArrayList;
import java.util.List;

import model.Abilities;

public class Disciplines extends Abilities {

	private List<String> disciplines = new ArrayList<String>();; 
	
	public Disciplines() {}
	
	

	public Disciplines(List<String> disciplines) {
		super();
		this.disciplines = disciplines;
	}



	public List<String> getDisciplines() {
		return disciplines;
	}

	public void setDisciplines(List<String> disciplines) {
		this.disciplines = disciplines;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("Disciplines [disciplines=");
		builder.append(disciplines);
		builder.append("]");
		return builder.toString();
	}

}
