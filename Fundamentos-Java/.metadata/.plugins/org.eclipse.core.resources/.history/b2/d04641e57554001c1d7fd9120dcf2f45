package model;

import interfaces.Expertises;
import interfaces.Knowledges;
import interfaces.Talents;

public class Abilities extends Character implements Talents, Expertises, Knowledges{

	private Talents talents;
	private Expertises expertises;
	private Knowledges knowledges;
	
	
	public Abilities(User user, String name, int age, String nature, String clan, boolean isNPC) {
		super(user, name, age, nature, clan, isNPC);
		// TODO Auto-generated constructor stub
	}


	public Talents getTalents() {
		return talents;
	}
	public void setTalents(Talents talents) {
		this.talents = talents;
	}

	public Expertises getExpertises() {
		return expertises;
	}
	public void setExpertises(Expertises expertises) {
		this.expertises = expertises;
	}

	public Knowledges getKnowledges() {
		return knowledges;
	}
	public void setKnowledges(Knowledges knowledges) {
		this.knowledges = knowledges;
	}

}
