package model;

public class Ficha extends Personagem{
	
	private int forca;
	private int agilidade;
	private int sorte;
	
	
	public Ficha(int forca, int agilidade, int sorte) {
		super();
		this.forca = forca;
		this.agilidade = agilidade;
		this.sorte = sorte;
	}
	
	
	public int getForca() {
		return forca;
	}
	public void setForca(int forca) {
		this.forca = forca;
	}
	public int getAgilidade() {
		return agilidade;
	}
	public void setAgilidade(int agilidade) {
		this.agilidade = agilidade;
	}
	public int getSorte() {
		return sorte;
	}
	public void setSorte(int sorte) {
		this.sorte = sorte;
	}
	
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("Ladino [forca=");
		builder.append(forca);
		builder.append(", agilidade=");
		builder.append(agilidade);
		builder.append(", sorte=");
		builder.append(sorte);
		builder.append("]");
		return builder.toString();
	}	
	
}
